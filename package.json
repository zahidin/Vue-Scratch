{
  "name": "vue-scratch",
  "version": "1.0.0",
  "description": "Boilerplate vue scratch",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "webpack --mode production --config webpack.config.js",
    "clean-build": "rm -rf dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zahidin/Vue-Scratch.git"
  },
  "author": "zahidin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zahidin/Vue-Scratch/issues"
  },
  "homepage": "https://github.com/zahidin/Vue-Scratch#readme",
  "dependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^3.4.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.1",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
